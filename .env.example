# For OpenAI
OPENAI_API_KEY=YOUR_OPENAI_API_KEY # Example - replace with real key
DSPY_LM_PROVIDER=openai # Example - replace with provider key
DSPY_LM_MODEL=gpt-5.1-codex # Example - replace with model

# For Anthropic
ANTHROPIC_API_KEY=sk-anth... # Example - replace with real key
DSPY_LM_PROVIDER=anthropic # Example - replace with real provider key
DSPY_LM_MODEL=claude-4.5-haiku # Example - replace with real model

# For Ollama (local AI)
DSPY_LM_PROVIDER=ollama # Example - replace with real provider
DSPY_LM_MODEL=qwen3 # Example - replace with real model
# OLLAMA_BASE_URL=http://localhost:11434/v1

# For OpenRouter (access multiple models)
OPENROUTER_API_KEY=sk-or-... # Example - replace with real key
DSPY_LM_PROVIDER=openrouter # Example - replace with real provider
DSPY_LM_MODEL=x-ai/grok-4.1-fast:free # Example - replace with real model

# For Google Gemini (https://aistudio.google.com/app/apikey)
# GOOGLE_API_KEY=...
# DSPY_LM_PROVIDER=google
# DSPY_LM_MODEL=gemini-1.5-pro

# For Cohere (https://dashboard.cohere.com/api-keys)
# COHERE_API_KEY=...
# DSPY_LM_PROVIDER=cohere
# DSPY_LM_MODEL=command-r-plus

# For Azure OpenAI (https://portal.azure.com/)
# AZURE_API_KEY=...
# AZURE_API_BASE=https://YOUR_RESOURCE_NAME.openai.azure.com/
# AZURE_API_VERSION=2024-02-15-preview
# DSPY_LM_PROVIDER=azure
# DSPY_LM_MODEL=YOUR_DEPLOYMENT_NAME

# For Together AI (https://api.together.xyz/settings/api-keys)
# TOGETHERAI_API_KEY=...
# DSPY_LM_PROVIDER=together
# DSPY_LM_MODEL=meta-llama/Llama-3-70b-chat-hf

# For Replicate (https://replicate.com/account/api-tokens)
